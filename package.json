{
  "name": "react-expander",
  "version": "0.0.1",
  "description": "Render expandable components with React.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir lib",
    "watch": "babel ./src --out-dir lib --source-maps inline --watch",
    "lint": "eslint src",
    "lint:test": "eslint --env mocha --env node test",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha test/unit/**/*.spec.js",
    "test:watch": "npm run test:unit -- --watch"
  },
  "keywords": [
    "react"
  ],
  "author": "ryym",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryym/react-expander"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-espower": "^2.1.1",
    "babel-plugin-module-alias": "^1.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "jsdom": "^8.0.1",
    "mocha": "^2.4.5",
    "power-assert": "^1.2.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "sinon": "^1.17.3"
  }
}
